{
  "name": "poseidon2-compression-ts",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^24.2.1",
    "tsup": "^8.5.0",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@zkpassport/poseidon2": "^0.6.2"
  },
  "scripts": {
    "build": "tsup index.ts --format esm,cjs --dts --out-dir dist --clean",
    "prepare": "npm run build"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false
}
